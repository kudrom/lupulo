{
    "distances": {
        "type": "list",
        "length": 8,
        "item_type": "number",
        "item_range": [0,4]
    },
    "leds": {
        "type": "list",
        "length": 8,
        "item_type": "enum",
        "item_values": ["on", "off"]
    },
    "battery": {
        "type": "percentage"
    },
    "date": {
        "type": "epoch"
    },
    "rotation": {
        "type": "number",
        "range": [0, 360]
    },
    "direction": {
        "type": "number",
        "range": [0, 100]
    },
    "acceleration": {
        "type": "number",
        "range": [0, 10]
    },
    "motor": {
        "type": "dict",
        "keys": ["speed", "turn_radius"],
        "speed_type": "number",
        "turn_type": "number"
    }
}
