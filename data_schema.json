{
    "distances": {
        "type": "list",
        "length": 8,
        "item_type": "number",
        "item_range": [0,4]
    },
    "leds": {
        "type": "list",
        "length": 8,
        "item_type": "enum",
        "item_values": ["on", "off", "null"]
    },
    "battery": {
        "type": "number",
        "range": [0, 100]
    },
    "date": {
        "type": "date",
        "variance": 60
    },
    "rotation": {
        "type": "number",
        "range": [0, 360]
    },
    "direction": {
        "type": "number",
        "range": [0, 100]
    },
    "acceleration": {
        "type": "number",
        "range": [0, 10]
    },
    "motor": {
        "type": "dict",
        "keys": ["speed", "turn_radius"],
        "speed_type": "number",
        "speed_range": [0,5],
        "turn_radius_type": "number",
        "turn_radius_range": [0,3]
    }
}
